        <port2>6633</port2>
        
        <ip3>10.254.1.203</ip3>
        <port3>6633</port3>
        
        <ip4>10.254.1.204</ip4>
        <port4>6633</port4>
        
        <ip5>10.254.1.205</ip5>
        <port5>6633</port5>
        
        <ip6>10.254.1.206</ip6>
        <port6>6633</port6> 
       
        <ip7>10.254.1.207</ip7>
        <port7>6633</port7>

    </CTRL>

    <MN>
        <ip1>10.254.1.200</ip1>
    </MN>

    <BENCH>
        <user>admin</user>
        <ip1>10.254.1.200</ip1>
    </BENCH>

    <JSON>
    </JSON>

